---
eleventyComputed:
  title: "{{ skill.title }}"
  summary: "{{ skill.summary }}"
  tag: "skill:{{ title | slugify }}"
---
{%- layout 'default' -%}
{%- block 'main' -%}
  {%- render 'header'
    title: title
    summary: summary
  -%}
  {%- assign items = collections.project | includes: 'data.tags',tag | reverse -%}
  {%- render 'items' with items
    component: 'card'
    gridSize: '12rem'
  -%}
{%- endblock -%}
