---
eleventyComputed:
  title: "{{ source.title }}"
  summary: "{{ source.summary }}"
  tag: "source:{{ title | slugify }}"
---
{%- layout 'default' -%}
{%- block 'main' -%}
  {%- render 'header'
    title: title
    summary: summary
    url: source.url
  -%}
  {%- assign items = collections.article | includes: 'data.tags',tag -%}
  {%- render 'items' with items
    modifier: 'inline'
  -%}
{%- endblock -%}
