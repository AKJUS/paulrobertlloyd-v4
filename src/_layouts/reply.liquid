---
layout: default.liquid
eleventyComputed:
  page_title: "Reply: {{ date | date: dates.date_time }}"
---
{%- render 'footer'
  date: date
  show_time: true
  time_zone: time_zone
  location: location
  geo: geo
  in_reply_to: in_reply_to
  canonical: canonical
  vocabulary: vocabulary
-%}
{%- render 'content' with content
  properties: 'e-content p-name'
-%}
{%- render 'meta'
  categories: category
  syndication: syndication
-%}
