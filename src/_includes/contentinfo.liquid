<footer class="contentinfo">
  <div class="contentinfo__container container">
    {%- render 'navigation' with navigation.social
      modifier: 'social'
    -%}
    {%- render 'navigation' with navigation.tertiary
      page: page
      tags: tags
    -%}
    <small class="contentinfo__legal">
      © <time>{{ 'now' | date: '%Y' }}</time>
    </small>
  </div>
</footer>
