<ul class="navigation" role="list">
{%- for item in navigation %}
  <li>
    <a {% if item.rel %}rel="{{ item.rel }}" {% endif %}href="{{ item.url }}"{% if type contains item.postType or page.url contains item.url %} aria-current="true"{% endif %}>
      {%- app_icon item.icon item.iconLabel 24 -%}
      {{- item.label -}}
    </a>
  </li>
{%- endfor %}
</ul>
