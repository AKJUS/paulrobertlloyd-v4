---
layout: event
pagination:
  data: events
  size: 1
  alias: event
  addAllPagesToCollections: true
permalink: "events/{{ event.start | date: '%Y/%m/%d' }}/{{ event.slug }}/"
tags:
  - event
sitemap:
  changefreq: monthly
  priority: 0.4
eleventyComputed:
  type_prefix: e
  vocab: event
  title: "{{ event.title }}"
  summary: "{{ event.summary }}"
  icon: "{{ event.icon }}"
  start: "{{ event.start }}"
  end: "{{ event.end }}"
  location:
    name: "{{ event.location.name }}"
  presented: "{{ event.presented }}"
---
