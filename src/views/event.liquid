---
layout: event
pagination:
  data: events
  size: 1
  alias: event
  addAllPagesToCollections: true
permalink: "{{ event.start | date: '%Y/%j' }}/{{ type_prefix }}1/{{ event.title | slugify }}/"
tags:
  - event
type_prefix: e
vocabulary: event
eleventyComputed:
  title: "{{ event.title }}"
  summary: "{{ event.summary }}"
  content: "{{ event.content }}"
  icon: "{{ event.icon }}"
  start: "{{ event.start }}"
  end: "{{ event.end }}"
  location:
    name: "{{ event.location.name }}"
  presented: "{{ event.presented }}"
---
