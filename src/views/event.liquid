---
layout: event.liquid
pagination:
  data: events
  size: 1
  alias: event
  addAllPagesToCollections: true
permalink: "{{ event.start | date: '%Y/%j' }}/{{ type_prefix }}1/{{ event.title | slugify }}/"
tags:
  - event
type: event
type_index: 1
type_prefix: e
vocabulary: event
eleventyComputed:
  title: "{{ event.title }}"
  summary: "{{ event.summary }}"
  icon: "{{ event.icon }}"
  start: "{{ event.start }}"
  end: "{{ event.end }}"
  location:
    name: "{{ event.location.name }}"
    locality: "{{ event.location.locality }}"
    country_name: "{{ event.location.country_name }}"
  presented: "{{ event.presented }}"
  properties:
    start: "{{ event.start }}"
    end: "{{ event.end }}"
    location:
      name: "{{ event.location.name }}"
      locality: "{{ event.location.locality }}"
      country_name: "{{ event.location.country_name }}"
---
{{ event.content | markdown }}
