{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:jsdoc/recommended",
    "plugin:unicorn/recommended",
    "prettier"
  ],
  "ignorePatterns": ["www/**"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["jsdoc", "unicorn", "prettier"],
  "rules": {
    "unicorn/filename-case": [
      "warn",
      {
        "case": "kebabCase",
        "ignore": ["eleventyComputed.js"]
      }
    ]
  }
}
